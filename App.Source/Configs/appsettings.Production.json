{
  "urls": "http://+:8080",
  "enableLogging": false,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Console": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      },
      "FormatterName": "json",
      "FormatterOptions": {
        "SingleLine" : true,
        "IncludeScopes": true,
        "TimestampFormat": "dd:MM:yyyy HH:mm:ss",
        "UseUtcTimestamp": false,
        "jsonWriterOption" : {
          "Indented" : true
        }
      }
    }
},
"AllowedHosts": "*",
"ProxySettings": {
  "Routes": {
    "route1": {
      "ClusterId": "cluster1",
      "Match": {
        "Path": "{**catch-all}"
      }
    }
  },
  "Clusters": {
    "cluster1": {
      "Destinations": {
        "destination1": {
          "Address": "https://example.com/"
        }
      }
    }
  }
}
}